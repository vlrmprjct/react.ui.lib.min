{
    "name": "@vlrm/library",
    "version": "1.0.0",
    "description": "React TypeScript UI Library NPM Package Minimal Setup",
    "author": "Thmomas Meschke @vlrmprjct",
    "private": false,
    "license": "ISC",
    "keywords": [
        "storybook",
        "react",
        "reactjs",
        "typescript",
        "ui-components",
        "library",
        "npm",
        "npm-package"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "!*.stories.*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vlrmprjct/react.ui.lib.min.git"
    },
    "bugs": {
        "url": "https://github.com/vlrmprjct/react.ui.lib.min/issues",
        "email": "mail@lokalnetz.com"
    },
    "scripts": {
        "build": "rm -rf dist && rm -rf *.tgz && webpack --config config/webpack.config.js",
        "build-storybook": "storybook build",
        "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
        "storybook": "storybook dev -p 6006",
        "start": "storybook dev -p 6006 --ci -c config/storybook watch-css"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.22.2",
        "@babel/preset-react": "^7.22.3",
        "@storybook/addon-essentials": "^7.0.18",
        "@storybook/addon-interactions": "^7.0.18",
        "@storybook/addon-links": "^7.0.18",
        "@storybook/addon-mdx-gfm": "^7.0.18",
        "@storybook/blocks": "^7.0.18",
        "@storybook/react": "^7.0.18",
        "@storybook/react-webpack5": "^7.0.18",
        "@storybook/testing-library": "^0.1.0",
        "@types/react": "^18.2.7",
        "@types/react-dom": "^18.2.4",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "babel-jest": "^29.5.0",
        "babel-loader": "^9.1.2",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "jest": "^29.5.0",
        "node-sass": "^9.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-test-renderer": "^18.2.0",
        "sass-loader": "^13.3.1",
        "storybook": "^7.0.18",
        "style-loader": "^3.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "typescript": "^5.0.4",
        "webpack": "^5.84.1",
        "webpack-cli": "^5.1.1"
    }
}
